
vendors = official
@keyframes rotate {
  0% {
    transform: rotateZ(0deg) 
  }  
  100% {
    transform: rotateZ(360deg) 
  }
}
@keyframes Rerotate {
  0% {
    transform: rotateZ(0deg) 
  }  
  100% {
    transform: rotateZ(-360deg) 
  }
}
@keyframes flicker {
  0% {
    opacity 0
  }  
  100% {
    opacity 1
  }
}

.fade-enter-active, .fade-leave-active {
  transition: opacity .5s;
}
.fade-enter, .fade-leave-to /* .fade-leave-active below version 2.1.8 */ {
  opacity: 0;
}
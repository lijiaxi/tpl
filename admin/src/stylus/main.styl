

.container {
  position: relative;
  padding: 0.1px;
  font-size: 14px;
  width: 100%
  height: 100vh
  background: #f4f4f4
  overflow-y: auto
  overflow-x: hidden
  box-sizing border-box
  -webkit-overflow-scrolling: touch
}

.grey_bar {
  width: 100%;
  padding: 5px 0;
}
.grey_line {
  width: 100%;
  padding-bottom: 1px;
  background: #f4f4f4
}
.dashLine {
  border-bottom: 1px dashed #999999
}
input {
  background: rgba(0, 0, 0, 0)
}

button {
  padding: 0;
  border: none;
}

